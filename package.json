        "axios": "^0.27.2",
        "clearbit": "^1.3.5",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^16.0.0",
        "express": "~4.16.1",
        "googleapis": "^100.0.0",
        "graphile-worker": "^0.12.2",
